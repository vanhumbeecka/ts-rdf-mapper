{
  "name": "ts-rdf-mapper",
  "version": "1.0.0",
  "description": "Object to Turtle mapper",
  "main": "./bin/src/main/index.js",
  "typings": "./bin/src/main/index.d.ts",
  "scripts": {
    "watch-compile": "node_modules/.bin/tsc -w -p .",
    "rollup": "rollup -c",
    "dist": "npm run build && npm run test && npm run rollup",
    "clean": "rm -r ./dist",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "RDF",
    "RDF Serializer",
    "RDF Deserializer",
    "TURTLE",
    "TTL",
    "ObjectMapper"
  ],
  "author": "Anton Sukhovatkin",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.0",
    "@types/jest": "^26.0.20",
    "jest": "^26.6.3",
    "rollup": "^2.40.0",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "@types/n3": "^1.4.4",
    "n3": "^1.8.1",
    "reflect-metadata": "^0.1.13"
  }
}
